  <style>
    /* Page Specific Styles */
    body {
      font-family: 'Poppins', sans-serif !important;
      background-color: #f9f9f9 !important;
    }

    .honeymoon-banner {
      position: relative !important;
      width: 100% !important;
      height: 60vh !important;
      background: url('images/romantic-explore/honeymoon-cover.webp') no-repeat center center/cover !important;
      display: flex !important;
      align-items: center !important;
      justify-content: center !important;
      color: white !important;
      text-align: center !important;
    }

    .honeymoon-banner::after {
      content: '' !important;
      position: absolute !important;
      top: 0 !important;
      left: 0 !important;
      right: 0 !important;
      bottom: 0 !important;
      background: rgba(0, 0, 0, 0.4) !important;
    }

    .banner-content {
      position: relative !important;
      z-index: 1 !important;
    }

    /* Splide Customization */
    .splide__slide {
      padding: 10px;
      /* Add spacing around cards */
      height: auto;
      /* Allow slide to determine height */
      display: flex;
      /* Ensure full height stretching */
    }

    .destination-card {
      border: none !important;
      border-radius: 15px !important;
      overflow: hidden !important;
      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1) !important;
      transition: transform 0.3s ease !important;
      background: white !important;
      width: 100% !important;
      display: flex !important;
      flex-direction: column !important;
      height: 100% !important;
    }

    .destination-card:hover {
      transform: translateY(-5px) !important;
    }

    .card-img-wrapper {
      position: relative !important;
      height: 220px !important;
      width: 100% !important;
      overflow: hidden !important;
      flex-shrink: 0 !important;
    }

    .card-img-wrapper img {
      width: 100% !important;
      height: 100% !important;
      object-fit: cover !important;
      transition: transform 0.5s ease !important;
    }

    .destination-card:hover .card-img-wrapper img {
      transform: scale(1.1) !important;
    }

    .card-body {
      padding: 1.5rem !important;
      text-align: center !important;
      display: flex !important;
      flex-direction: column !important;
      justify-content: space-between !important;
      flex-grow: 1 !important;
      background-color: #fff !important;
    }

    .card-title {
      font-weight: 700 !important;
      color: #333 !important;
      margin-bottom: 0.5rem !important;
    }

    .btn-explore {
      background-color: #ff4757 !important;
      color: white !important;
      border-radius: 25px !important;
      padding: 8px 25px !important;
      text-decoration: none !important;
      transition: background 0.3s !important;
      display: inline-block !important;
      margin-top: auto !important;
      align-self: center !important;
      border: none;
    }

    .btn-explore:hover {
      background-color: #ff6b81 !important;
      color: white !important;
    }

    /* Customize Splide Arrows */
    .splide__arrow {
      background: white !important;
      opacity: 1 !important;
      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2) !important;
      width: 40px !important;
      height: 40px !important;
    }

    .splide__arrow svg {
      fill: #ff4757 !important;
      height: 1.5em !important;
      width: 1.5em !important;
    }

    .splide__pagination__page.is-active {
      background: #ff4757 !important;
      transform: scale(1.2);
    }
  </style>
</head>

