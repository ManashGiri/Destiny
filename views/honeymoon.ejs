            <form>
              <div class="mb-3">
                <label for="name" class="form-label fs-5 first-para">Your Name *</label>
                <div class="input-group">
                  <span class="input-group-text"><i class="fas fa-user"></i></span>
                  <input type="text" class="form-control" id="name" placeholder="Enter your name" />
                </div>
              </div>
              <div class="mb-3">
                <label for="email" class="form-label fs-5 first-para">Email Id *</label>
                <div class="input-group">
                  <span class="input-group-text"><i class="fas fa-envelope"></i></span>
                  <input type="email" class="form-control" id="email" placeholder="Enter your Email Id" />
                </div>
              </div>
              <div class="mb-3">
                <label for="phone" class="form-label fs-5 first-para">Phone *</label>
                <div class="input-group">
                  <span class="input-group-text"><i class="fas fa-phone"></i></span>
                  <input type="tel" class="form-control" id="phone" placeholder="Enter your 10 digit number" />
                </div>
              </div>
              <div class="mb-3">
                <label for="message" class="form-label fs-5 first-para">Message</label>
                <div class="input-group">
                  <span class="input-group-text d-flex align-items-start pt-2"><i class="fas fa-comment"></i></span>
                  <textarea class="form-control" id="message" rows="3" placeholder="Write your message"></textarea>
                </div>
              </div>
              <button type="submit" class="btn btn-yellow w-100">
                Submit
              </button>
            </form>
          </div>
        </div>
      </div>
    </div>
